{
  "version": 2,
  "builds": [
    {
      "src": "src/index.tsx",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.tsx"
    }
  ],
  "env": {
    "WEBROOT": "",
    "ALLOW_UNAUTHENTICATED": "false",
    "JWT_SECRET": "@jwt_secret",
    "DATABASE_URL": "@database_url",
    "MAX_CONVERT_PROCESS": "3",
    "UPLOADS_DIR": "./uploads",
    "OUTPUT_DIR": "./output"
  }
}
}
}
